{
  "expo": {
    "name": "Movie Rating",
    "slug": "movie-rating",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bgless-icon.png",
        "backgroundColor": "#1a1a40"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {},
    "description": "A React Native, NestJS, GraphQL & Prisma project. Discover movies, add your favorite to your list with a review & rating, find out other people's list.",
    "githubUrl": "https://github.com/whygee-dev/movierating"
  }
}
