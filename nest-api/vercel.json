{
  "version": 2,
  "env": {
    "DATABASE_URL": "@database_url",
    "jwtSecret": "@jwt_secret",
    "TMDB_KEY": "@tmdb_key"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
